/* pages/feedback/feedback.wxss */
.require::after {
    content: '*';
    color: red;
    margin-left: 3rpx;
}
.title_num_box{
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.title{
    color: #000E1A;
    font-weight: bold;
font-size: 28rpx;

}
.num{
    color: #979797;
    font-size: 24rpx;
}

.text_area{
    margin-top: 32rpx;
    width: 100%;
height: 268rpx;
background: #F4F4F4;
border-radius: 8rpx 8rpx 8rpx 8rpx;
opacity: 1;
padding: 20rpx;
box-sizing: border-box;
font-size: 24rpx;
}
.placeholder{
    font-size: 24rpx;
    color: #979797;
}
.van-uploader{
    margin-top: 32rpx;
}

.contact_area{
    margin-top: 32rpx;
    width: 100%;
height: 80rpx;
line-height: 80;
background: #F4F4F4;
border-radius: 8rpx 8rpx 8rpx 8rpx;
opacity: 1;
box-sizing: border-box;
padding: 0rpx 32rpx;
}
.bottom_fun{
    width: 100%;
    background-color: #fff;
    position: fixed;
    bottom: 0;
    padding: 20rpx 68rpx 48rpx 68rpx;
    box-sizing: border-box;
}
.funbtn{
    display: flex;
    justify-content: space-between;
    width: 100%;
}
.contact{
    width: 654rpx;
    height: 96rpx;
    background: #202020;
    border-radius: 8rpx 8rpx 8rpx 8rpx;
    opacity: 1;
    font-size: 28rpx;
    line-height: 96rpx;
    color: #fff;
    text-align: center;
}
