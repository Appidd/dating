/* pages/login/login.wxss */
page {
    background-color: #FFFFFF;
}

.logo_img {
    margin-top: 140rpx;
    width: 258rpx;
    height: 258rpx;
    background: red;
    border-radius: 0rpx 0rpx 0rpx 0rpx;
}

.logo_title {
    font-size: 40rpx;
    font-weight: bold;
    color: #333333;
    line-height: 47rpx;
    margin-top: 32rpx;
}

.jurisdiction_des {
    box-sizing: border-box;
    width: 100%;
    padding: 0rpx 96rpx;
    margin-top: 200rpx;
}

.jurisdiction_title {
    height: 40rpx;
    font-size: 28rpx;
    font-weight: bold;
    color: #333333;
    line-height: 33rpx;
}

.jurisdiction_detail {
    /* height: 34rpx; */
    font-size: 24rpx;
    font-weight: 500;
    color: #898A8D;
    line-height: 28rpx;
    margin-top: 26rpx;
    padding-left: 24rpx;
    position: relative;
    /* background-color: aquamarine; */
}
.jurisdiction_detail::before {
content: '';
position: absolute;
top: 50%;
width: 8rpx;
height: 8rpx;
background: #898A8D;
opacity: 1;
border-radius: 50%;
transform: translate(50%,-50%);
left: 0;
}
.bottom_fix{
    position: fixed;
    bottom: 60rpx;
    width: 100%;
    box-sizing: border-box;
    padding: 0rpx 48rpx;
}
.login_btn{
    height: 96rpx;
    width: 100%;
    background-color: #08C160;
    border-radius: 60rpx;
    color: #fff;
    font-size: 28rpx;
}
.wechat_img{
    height: 54rpx;
    width: 54rpx;
    margin-right: 12rpx;
}